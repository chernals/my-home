#!/bin/sh
geant4_envbindir="/usr/local/Cellar/geant4/10.3.3/bin"

if test "x$DYLD_LIBRARY_PATH" = "x" ; then
  export DYLD_LIBRARY_PATH=/usr/local/Cellar/clhep/2.3.4.4/lib
else
  export DYLD_LIBRARY_PATH=${DYLD_LIBRARY_PATH}:/usr/local/Cellar/clhep/2.3.4.4/lib
fi

if test "x$PATH" = "x" ; then
  export PATH="$geant4_envbindir"
else
  export PATH="$geant4_envbindir":${PATH}
fi


if test "x$DYLD_LIBRARY_PATH" = "x" ; then
  export DYLD_LIBRARY_PATH="`cd $geant4_envbindir/../lib > /dev/null ; pwd`"
else
  export DYLD_LIBRARY_PATH="`cd $geant4_envbindir/../lib > /dev/null ; pwd`":${DYLD_LIBRARY_PATH}
fi


export G4NEUTRONHPDATA="`cd $geant4_envbindir/../share/Geant4-10.3.3/data/G4NDL4.5 > /dev/null ; pwd`"
export G4LEDATA="`cd $geant4_envbindir/../share/Geant4-10.3.3/data/G4EMLOW6.50 > /dev/null ; pwd`"
export G4LEVELGAMMADATA="`cd $geant4_envbindir/../share/Geant4-10.3.3/data/G4PhotonEvaporation4.3.2 > /dev/null ; pwd`"
export G4RADIOACTIVEDATA="`cd $geant4_envbindir/../share/Geant4-10.3.3/data/G4RadioactiveDecay5.1.1 > /dev/null ; pwd`"
export G4NEUTRONXSDATA="`cd $geant4_envbindir/../share/Geant4-10.3.3/data/G4NEUTRONXS1.4 > /dev/null ; pwd`"
export G4PIIDATA="`cd $geant4_envbindir/../share/Geant4-10.3.3/data/G4PII1.3 > /dev/null ; pwd`"
export G4REALSURFACEDATA="`cd $geant4_envbindir/../share/Geant4-10.3.3/data/RealSurface1.0 > /dev/null ; pwd`"
export G4SAIDXSDATA="`cd $geant4_envbindir/../share/Geant4-10.3.3/data/G4SAIDDATA1.1 > /dev/null ; pwd`"
export G4ABLADATA="`cd $geant4_envbindir/../share/Geant4-10.3.3/data/G4ABLA3.0 > /dev/null ; pwd`"
export G4ENSDFSTATEDATA="`cd $geant4_envbindir/../share/Geant4-10.3.3/data/G4ENSDFSTATE2.1 > /dev/null ; pwd`"
